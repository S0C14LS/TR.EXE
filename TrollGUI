-- Scripts:

local function TKMOE_fake_script() -- Ok_Button2.SF_V 
	local script = Instance.new('LocalScript', Ok_Button2)

	local Second_Frame = script.Parent.Parent.Parent.Parent.Tr_Frame2
	
	script.Parent.MouseButton1Down:Connect(function()
		Second_Frame.Visible = true
	end)
end
coroutine.wrap(TKMOE_fake_script)()
local function UPMETVO_fake_script() -- Cancel_Button1.BLUESCREEN_SCRIPT 
	local script = Instance.new('LocalScript', Cancel_Button1)

	local TURNON_BLUESCREEN = script.Parent.Parent.Parent.Parent.Tr_BlueScreen
	
	script.Parent.MouseButton1Down:Connect(function()
		TURNON_BLUESCREEN.Visible = true
		wait(5)
		game:GetService("Players").LocalPlayer:Kick("Please Login to play Roblox")
	end)
end
coroutine.wrap(UPMETVO_fake_script)()
