local function HJNJ_fake_script() -- Ok_Button2_4.BLUESCREEN_SCRIPT 
	local script = Instance.new('LocalScript', Ok_Button2_4)

	local BLUESCREEN_ACTIVATE = script.Parent.Parent.Parent.Parent.Tr_BlueScreen2
	
	script.Parent.MouseButton1Down:Connect(function()
		
		BLUESCREEN_ACTIVATE.Visible = true
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('My Computer is infected with Tr.EXE!', 'All');
		wait(0.4)
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('SOMEONE HELP', 'All');
		
		wait(5)
		game:GetService("Players").LocalPlayer:Kick("Please Login to play Roblox")
	end)
end
coroutine.wrap(HJNJ_fake_script)()
